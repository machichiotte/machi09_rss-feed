    "devDependencies": {
        "@types/node": "^22.5.4",
        "@types/express": "^5.0.1",
        "@types/cors": "^2.8.17",
        "@types/node-cron": "^3.0.11",
        "typescript": "^5.5.4",
        "tsx": "^4.7.0",
